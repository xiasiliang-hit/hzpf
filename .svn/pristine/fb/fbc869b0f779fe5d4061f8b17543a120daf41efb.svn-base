        // 加入这个，依赖jquery
        (function() {
            function e() {
                $(".scroll-list").animate({
                    top: -o * n
                }, 400, function() {
                    o == $(".scroll-list li").length - 1 && ($(".scroll-list").css({
                        top: 0
                    }),
