    <insert id="insert" parameterClass="userClaimApply">
        <![CDATA[
			INSERT INTO pf_user_claim_apply(
		gmt_create,gmt_modified,
		concur_plan_id,user_concur_relation_id,user_id,user_name,verify_time,verify_user,verify_user_msg,verify_tel,verify_hospital,verify_event_id,verify_user_result,verify_result,pic_array,descr,follow_user_name,follow_user_tel,status,result,attach		,pre_collect_money,collect_money,publish_start_day,dunk_day,bandy_start_day,event_end_day,event_submit_day,month_index,join_user_count,pre_join_user_count,bank_proof_pic
		)
			VALUES(NOW(),NOW(),#concurPlanId#,#userConcurRelationId#,#userId#,#userName#,#verifyTime#,#verifyUser#,#verifyUserMsg#,#verifyTel#,#verifyHospital#,#verifyEventId#,#verifyUserResult#,#verifyResult#,#picArray#,#descr#,#followUserName#,#followUserTel#,#status#,#result#,#attach#
			,#preCollectMoney#,#collectMoney#,#publishStartDay#,#dunkDay#,#bandyStartDay#,#eventEndDay#,#eventSubmitDay#,#monthIndex#,#joinUserCount#,#preJoinUserCount#,#bankProofPic#
			)
		]]>
        <selectKey keyProperty="id" resultClass="Long">
            <![CDATA[
			SELECT LAST_INSERT_ID() AS value
		]]>
        </selectKey>
    </insert>


